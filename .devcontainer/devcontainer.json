{
  "name": "containers",
  "dockerComposeFile": [
    "../docker-compose.yaml"
  ],
  "service": "dev-container",
  "workspaceFolder": "/workspaces/containers",
  "postCreateCommand": "cd $WORKSPACE/",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.profiles.linux": {
          "bash": {
            "path": "bash",
            "icon": "terminal-bash"
          }
        }
      },
      "extensions": [
        "ms-azuretools.vscode-docker",
        "github.vscode-github-actions",
        "yzhang.markdown-all-in-one"
      ]
    }
  }
}